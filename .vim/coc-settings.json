{
  "suggest.noselect": false,
  "languageserver": {
    "clojure-lsp": {
      "command": "bash",
      "args": [
        "-c",
        "clojure-lsp"
      ],
      "filetypes": [
        "clojure",
        "clojurescript"
      ],
      "rootPatterns": [
        "project.clj"
      ],
      "additionalSchemes": [
        "jar",
        "zipfile"
      ],
      "trace.server": "verbose",
      "initializationOptions": {}
    }
  },
  "markdownlint.config": {
    "default": true,
    "line_length": false
  }
}
